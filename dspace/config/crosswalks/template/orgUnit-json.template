	"identifiers": [
		"@organization.identifier@",
	],
	@if.metadata.organization-identifier.end@
	@if.metadata.oairecerif-identifier-url.start@
	"url-identifiers": [
		"@oairecerif.identifier.url@",
	],
	@if.metadata.oairecerif-identifier-url.end@
	@if.metadata.organization-identifier-ruc.start@
    "ruc-identifiers": [
        "@organization.identifier.ruc@",
    ],
    @if.metadata.organization-identifier-ruc.end@
    @if.metadata.organization-identifier-rin.start@
    "rin-identifiers": [
        "@organization.identifier.rin@",
    ],
    @if.metadata.organization-identifier-rin.end@
    @if.metadata.organization-identifier-ror.start@
    "ror-identifiers": [
        "@organization.identifier.ror@",
    ],
    @if.metadata.organization-identifier-ror.end@
