    @group.perucris-renacyt-registration.start@
    {
        "registration": "@perucris.renacyt.registration@",
        "registration-number": "@perucris.renacyt.registrationNumber@",
        "classification": "@perucris.renacyt.classification@",
        "ocde": "@virtual.mapConverter.ocdeSubjectUris.perucris-renacyt-ocde@",
        "strength": "@perucris.renacyt.strength@",
        "date-of-qualification": "@perucris.renacyt.dateOfQualification@",
