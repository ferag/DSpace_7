{
	"name": "@dc.title@",
	"alternativeName": [
	    "@dc.title.alternative@",
	],
	"legalName": "@organization.legalName@",
	"acronym": "@oairecerif.acronym@",
	"type": "@dc.type@",
	"parentOrganization": {
		@relation.organization-parentOrganization.start@
		"name": "@dc.title@",
		"acronym": "@oairecerif.acronym@"
		@relation.organization-parentOrganization.end@
