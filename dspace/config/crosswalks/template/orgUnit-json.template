    "scopusaff-identifiers": [
        "@organization.identifier.scopusaffid@",
    ],
    @if.metadata.organization-identifier-scopusaffid.end@
    @if.metadata.organization-identifier-crossrefid.start@
    "crossref-identifiers": [
        "@organization.identifier.crossrefid@",
    ],
    @if.metadata.organization-identifier-crossrefid.end@
    @if.metadata.perucris-subject-ocde.start@
    "subjects": [
        "@virtual.mapConverter.ocdeSubjectUris.perucris-subject-ocde@",
    ],
    @if.metadata.perucris-subject-ocde.end@
    @if.metadata.dc-subject.start@
    "keywords": [
        "@dc.subject@",
    ],
    @if.metadata.dc-subject.end@
    "postAddress": {
        "locality": "@organization.address.addressLocality@",
        "country": "@organization.address.addressCountry@"
